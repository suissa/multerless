{
  "name": "nexmulter",
  "version": "1.0.0",
  "description": "Native Node.js multipart/form-data parser - Drop-in replacement for Multer with superior performance and zero dependencies",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./storage": {
      "import": "./dist/storage/index.js",
      "types": "./dist/storage/index.d.ts"
    }
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "node --test test/*.test.js",
    "test:coverage": "c8 node --test test/*.test.js",
    "benchmark": "node benchmarks/compare-multer.js",
    "prepublishOnly": "npm run build && npm test",
    "clean": "rimraf dist"
  },
  "keywords": [
    "multer",
    "multipart",
    "form-data",
    "file-upload",
    "native",
    "zero-dependencies",
    "performance",
    "typescript",
    "node.js",
    "express",
    "fastify"
  ],
  "author": {
    "name": "suissAI",
    "email": "suissaidev@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/suissa/nexmulter.git"
  },
  "bugs": {
    "url": "https://github.com/suissa/nexmulter/issues"
  },
  "homepage": "https://github.com/suissa/nexmulter#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "typescript": "^5.7.2",
    "c8": "^10.1.2",
    "rimraf": "^6.0.1"
  },
  "peerDependencies": {
    "express": ">=4.0.0"
  },
  "peerDependenciesMeta": {
    "express": {
      "optional": true
    }
  },
  "dependencies": {
    "mime-db": "^1.54.0",
    "mime-types": "^3.0.0",
    "negotiator": "^1.0.0",
    "accepts": "^2.0.0",
    "bytes": "~3.1.2",
    "content-type": "^1.0.5",
    "ms": "^2.1.3",
    "debug": "^4.4.3",
    "depd": "~2.0.0",
    "inherits": "~2.0.4",
    "setprototypeof": "~1.2.0",
    "statuses": "^2.0.1",
    "toidentifier": "~1.0.1",
    "http-errors": "^2.0.1",
    "safer-buffer": ">= 2.1.2 < 3.0.0",
    "iconv-lite": "~0.7.0",
    "ee-first": "1.1.1",
    "on-finished": "^2.4.1",
    "es-errors": "^1.3.0",
    "object-inspect": "^1.13.3",
    "side-channel-list": "^1.0.0",
    "function-bind": "^1.1.2",
    "call-bind-apply-helpers": "^1.0.1",
    "es-define-property": "^1.0.1",
    "es-object-atoms": "^1.0.0",
    "gopd": "^1.2.0",
    "dunder-proto": "^1.0.1",
    "get-proto": "^1.0.1",
    "has-symbols": "^1.1.0",
    "hasown": "^2.0.2",
